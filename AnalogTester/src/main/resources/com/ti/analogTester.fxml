<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="border" prefHeight="202.0" prefWidth="273.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.ti.control.AnTeViewController">
  <center>
    <VBox fx:id="chartHBox" />
  </center>
  <left>
    <VBox alignment="TOP_CENTER" prefHeight="202.0" prefWidth="157.0">
      <children>
        <TextField fx:id="portname" prefWidth="66.0" />
        <Button mnemonicParsing="false" onAction="#reopen" text="ReopenPort" />
        <Button mnemonicParsing="false" onAction="#changeState" text="EmulateStateChange" />
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>
  </left>
  <bottom>
    <fx:include fx:id="controlPanel"  source="ControlPanel.fxml"/>
  </bottom>
</BorderPane>
